  "author": "@augnustin && @gaut-b",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "cleanup": "rm -rf .cache dist build",
    "watch-app": "parcel serve app/index.html --dist-dir build --port 1234",
    "watch-server": "nodemon --watch server server/index.js",
    "dev": "concurrently \"npm run watch-app\" \"npm run watch-server\"",
    "build": "parcel build app/index.html --dist-dir build --public-url ./",
    "server": "node server/index.js",
    "start": "npm install && npm run build && npm run server",
    "test": "jest --watch *.test.js"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "bulma": "^1.0.2",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "lodash.get": "^4.4.2",
    "marked": "^16.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "reselect": "^5.1.1",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.1",
    "uuid": "^13.0.0",
    "parcel": "^2.16.0",
    "sass": "^1.79.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
